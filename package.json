{
  "name": "api-test-automation-playwright",
  "version": "1.0.0",
  "description": "Esse é um projeto de automação de testes usando playwright como foco principal para estudos.",
  "main": "playwright.config.js",
  "scripts": {  
    "testAll": "npx playwright test",
    "testAuth": "npx playwright test tests/auth.spec.js --headed",
    "testChrome": "npx playwright test tests/auth.spec.js --headed --project=chromium",
    "runnerUi": "npx playwright test tests/auth.spec.js --headed --project=chromium --ui",
    "report": "npx playwright show-report"  
  },
    "directories": {
    "test": "tests"
  },
  "dependencies": {
    "playwright": "^1.56.1",
    "playwright-core": "^1.56.1",
    "undici-types": "^7.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.5.2"
  },
  "author": "",
  "license": "ISC"
}
